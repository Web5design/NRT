<table>
  <caption>{{indicatorTitle}}</caption>
  <thead>
    <tr>
      <th>{{xAxis}}</th>
      <th>{{yAxis}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each dataRows}}
      <tr>
        <td class="center">{{this.formatted.x}}</td>
        <td class="center">{{this.formatted.y}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
